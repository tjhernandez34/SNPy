<div class="partial_form clear-fix center">
<%= form_for User.new do |f| %>
	<h2 class="h2">SIGN UP</h2>
	<div>
		<%= f.text_field :first_name, :placeholder => "First Name", :class => "form_field" %>
	</div>
	<div>
		<%= f.text_field :last_name, :placeholder => "Last Name", :class => "form_field" %>
	</div>
	<div>
		<%= f.email_field :email, :placeholder => "Email", :class => "form_field" %>
	</div>
	<div>
		<%= f.text_field :username, :placeholder => "Username", :class => "form_field" %>
	</div>

	<div>
		<%= f.password_field :password, :placeholder => "Password", :class => "form_field" %>
	</div>
	<div>
	<%= f.submit "SUBMIT", :class => "btn" %>
	</div>

<% end %>
</div>

